<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.rbe.rbe2001.fieldsimulator.gui.RobotActionScreenController">
   <children>
      <Label fx:id="actionLabel" layoutX="354.0" layoutY="114.0" prefHeight="390.0" prefWidth="491.0" text="Robot Action Description HERE" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Button fx:id="done" layoutX="488.0" layoutY="590.0" mnemonicParsing="false" onMouseClicked="#donePressed" prefHeight="95.0" prefWidth="220.0" text="Done" textAlignment="CENTER">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
      <Label fx:id="ErrorWarning" layoutX="332.0" layoutY="760.0" prefHeight="66.0" prefWidth="587.0" text="Please Place The Bin On The Cleat" textFill="#f50606">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="returnBTN" layoutX="486.0" layoutY="492.0" mnemonicParsing="false" onMouseClicked="#returnPressed" prefHeight="95.0" prefWidth="220.0" text="Return">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
